{% extends "layout.njk" %}

{% block content %}
<div class="card">
    <table>
        <thead>
            <tr>
                <th>Instructor</th>
                <th>Atendimento</th>
            </tr>
        </thead>
        <tbody>
            {% for Instructor in Instructors %}
            <tr>
                <td>{{ instructor.name }}</td>
                <td>{{ instructor.services }}</td>
            </tr>
            {% endfor %}
        </tbody>
    </table>
</div>
 
{% endblock content %}